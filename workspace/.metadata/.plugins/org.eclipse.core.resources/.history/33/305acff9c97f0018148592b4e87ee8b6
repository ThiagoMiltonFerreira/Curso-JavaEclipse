package ControleDeEstoque;

import java.util.Random;

public class Produtos {
	
	Produtos(int id,String codigoDeBarras,String chip){
		//System.out.print("ID: "+id+"\n"+"Codigo de barras:"+codigoDeBarras+"\n"+"Produto: "+chip);
		//String codigoDoProduto=codigoDeBarras;
		String CodBarrasProdutosEnome[][] = {{"1212","SDS 3k"}};
		if(CodBarrasProdutosEnome[0][1]==codigoDeBarras) 
		{
			System.out.println("ssssssss");
		}
			for(int contLinha=0;contLinha<1;contLinha++)
			{
				
				for(int contColuna=1;contColuna<=2;contColuna++)
				{
					
					System.out.print(CodBarrasProdutosEnome[contLinha][contColuna]+" ");
					
				
			}
		}
	}
}

	
	

